program TestModules
begin
    print("Testing Module Scaffolding...")
    print("MyUtils.double(5) = " + str(MyUtils.double(5)))
    print("MyUtils.Inner.triple(5) = " + str(MyUtils.Inner.triple(5)))

    define module MyUtils
    begin
        define function double(x)
        begin
            return x * 2
        end

        define module Inner
        begin
            define function triple(x)
            begin
                return x * 3
            end
        end
    end
end
