module JSON

/**
 * Converts a value to a JSON string.
 */
define function stringify(val: Any) : String
begin
    return Sys.jsonStringify(val)
end

/**
 * Parses a JSON string into a Vorlang value (Map, Array, etc).
 */
define function parse(s: String) : Any
begin
    return Sys.jsonParse(s)
end

end module
