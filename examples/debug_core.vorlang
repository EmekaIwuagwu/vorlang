import core
import io

program DebugCore
begin
    IO.println("Checking isNumber...")
    if isNumber(10) then
        IO.println("isNumber found and returned true")
    else
        IO.println("isNumber found and returned false")
    end if

    IO.println("Checking toBool...")
    if toBool(null) then
        IO.println("toBool found")
    else
        IO.println("toBool found (false)")
    end if
    
    IO.println("Checking isNull...")
    if isNull(null) then
       IO.println("isNull found (true)")
    else
       IO.println("isNull found (false)")
    end if
end
