program TestModules
begin
    define module MyUtils
    begin
        define function double(x)
        begin
            return x * 2
        end

        define function triple(x)
        begin
            return x * 3
        end
    end

    print("Testing Module Scaffolding...")
    print("MyUtils.double(5) = " + str(MyUtils.double(5)))
    print("MyUtils.triple(5) = " + str(MyUtils.triple(5)))
end
